{
	"expo": {
		"name": "my-app",
		"slug": "my-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"backgroundColor": "#16171D",
		"splash": {
			"image": "./assets/images/splach.png",
			"resizeMode": "contain",
			"backgroundColor": "#16171D"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-font",
				{
					"fonts": ["./assets/fonts/FiraSans-Regular.ttf", "./assets/fonts/FiraSans-SemiBold.ttf"]
				}
			],
			"expo-router"
		],
		"scheme": "purpleschool"
	}
}
